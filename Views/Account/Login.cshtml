@model KRWTransfer.Models.Login

<h1>Login</h1>

@using (Html.BeginForm("Login", "Account", FormMethod.Post))
{
    @Html.ValidationSummary();

    <span>Login: </span>
    @Html.TextBoxFor(x => x.Username)
    <span>Password: </span>
    @Html.PasswordFor(x => x.Password)
    <button type="submit">Login</button>
}